// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id String @id
  created_at DateTime @default(now())
  email String @unique
  password String
  name String
}

model Timer {
  id String @id
  timer String
  audio String
}